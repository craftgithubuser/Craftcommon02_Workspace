# Prompt the user to enter their test score
try:
    score = float(input("Enter your test score: "))
except ValueError:
    print("Invalid input. Please enter a numeric value.")
    exit()

# Use conditional statements to determine the grade
if score >= 90:
    grade = 'A'
elif score >= 80:
    grade = 'B'
elif score >= 70:
    grade = 'C'
elif score >= 60:
    grade = 'D'
else:
    grade = 'F'

# Display the calculated grade to the user
print(f"Your grade is: {grade}")
